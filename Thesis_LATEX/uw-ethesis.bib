issn = {0034-6748},
journal = {Review of scientific instruments},
keywords = {Coils ; Computer Science ; Diagnostic systems ; Integrators ; Magnetic fields ; Magnetic flux ; Modeling and Simulation ; Plasma ; Plasma currents ; Sensors ; Tokamak devices},
