@article{westinterfero,
issn = {0920-3796},
journal = {Fusion engineering and design},
keywords = {Algorithms ; Analysis ; Beams (radiation) ; Coils ; Diagnostic systems ; Digital electronics ; Electronics ; FIR diagnostic ; Interferometry ; Physics ; Polarimeters ; Polarimetry ; Ports ; Real-time control ; Real-time systems ; Spatial resolution ; Tokamak ; Tokamak devices},
language = {eng},
abstract = {•Ten channels with common retroreflectors and windows were repositioned in the WEST tokamak environment for a 0.1 normalized radius resolution.•Inner vessel mirrors close to the divertor allow the X point and edge plasma diagnose.•Digital electronics replaces the initial data processing electronics.•Data were routinely and trouble free produced for real time control at 1 ms sampling time and post pulse analysis with mixed 1 MHz and 1 ms sampling time during the 2 year first WEST plasma campaign.
In the frame of the WEST tokamak upgrade project, the Tore Supra combined interferometer-polarimeter diagnostic has been modified to fit the geometry and constraints induced by the implementation of divertor coils in the vacuum vessel. For a good spatial resolution of the plasma, as the vertical ports are now partially obstructed, 8 infrared beams go through a common horizontal port and are retro-reflected on an inner panel and 2 vertical port retro-reflected channels, with specific vacuum mirrors close to the divertor, could be implemented to diagnose the edge plasma.
The electronics and the data acquisition have also been renewed to improve the reliability and precision of the measurements. Novel digital electronics with embedded interferometric and polarimetric algorithms produce 1 ms data outputs for real time control.
The diagnostic has routinely been operated during the first WEST campaigns. In this article, we detail the new arrangement and discuss the first results with plasma.},
author = {Gil, C. and Colledani, G. and Domenes, M. and Volpe, D. and Berne, A. and Faisse, F. and Guillon, C. and Morales, J. and Moreau, P. and Santraine, B.},
